{
    "project_name": "My Awesome Faust Project",
    "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
    "description": "My Awesome Faust Project!",
    "long_description": "My Awesome Faust Project!",
    "author_name": "Marcos Schroh",
    "author_email": "{{ cookiecutter.author_name.lower()|replace(' ', '-') }}@gmail.com",
    "version": "0.1.0",
    "open_source_license": [
      "MIT",
      "BSD",
      "GPLv3",
      "Apache Software License 2.0",
      "Not open source"
    ],
    "use_pycharm": "n",
    "use_docker": "y",
    "include_docker_compose": "y",
    "include_page_view_tutorial": "y",
    "worker_port": 6066,
    "kafka_server_environment_variable": "KAFKA_BOOTSTRAP_SERVER",
    "include_codec_example": "y",
    "faust_loglevel": [
      "INFO",
      "CRITICAL",
      "ERROR",
      "WARNING",
      "DEBUG",
      "NOTSET"
    ],
    "include_schema_registry": "n"
}